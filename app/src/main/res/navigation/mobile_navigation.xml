<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication_test1.ui.home.HomeFragment"
        android:label="@string/title_tab1"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.myapplication_test1.ui.dashboard.DashboardFragment"
        android:label="@string/title_tab2"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_appointments"
        android:name="com.example.myapplication_test1.ui.appointments.AppointmentsFragment"
        android:label="@string/title_tab3"
        tools:layout="@layout/fragment_appointments" />
</navigation>